{
    "title": "easy.sh",
    "author": "easy.sh",
    "description": "轻松自定义shell命令/别名，并在任意服务器上使用",
    "language": "zh-hans",
    "plugins": [
	    "-lunr", 
	    "-search", 
	    "search-pro",
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "sharing-plus",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "sharing": {
            "douban": true,
            "facebook": true,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": false,
            "stumbleupon": false,
            "twitter": true,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "douban", "facebook", "qq", "weibo", "twitter"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        }
    }
}